
public class Prog3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		float a=1.27f;
		float b=3.881f;
		float c=9.6f;
		
		int x=(int)(a+b+c);
		int y=(int)Math.round((a+b+c));

		System.out.println("Sum as integer by type casting "+x);
		System.out.println("Sum as integer by math round "+y);


	}

}
